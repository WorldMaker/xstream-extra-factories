{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "strictFunctionTypes": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "declaration": true,
        "outDir": "./dist/",
        "downlevelIteration": true,
        "lib": [
            "es2015",
            "es2015.iterable",
            "esnext.asynciterable"
        ],
        "plugins": [
            { "name": "tslint-language-service" }
        ]
    },
    "files": [
        "index.ts"
    ]
}